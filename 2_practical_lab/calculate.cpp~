#include <iostream>
#include <cmath>

using namespace std;

int main() {
    int M; cout << "Please input integer number: "; cin >> M; cout << "\n";
    int n = M % 5;

    switch (n) {
        case 1:
            cout << (n / sqrt(pow(n, 5))) + abs(pow(n, 8) + 5);
            break;

        case 2:
            cout << atan(n + 2);
            break;

        case 3:
            cout << atan(n + 2);
            break;

        case 4:
            cout << sin(n + M_PI / 4);
            break;

        case 5:
            cout << exp(n)
            break;

        default:
            cout << 0;
    } 

    cout << "\n";

    return 0;
}
